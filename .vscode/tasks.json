{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "docker build booking service",
            "type": "shell",
            "command": "docker build -t acrappsaks.azurecr.io/booking-service:v0.2.0 .",
            "options": {
                "cwd": "${workspaceFolder}/src/booking_service/src"
            }
        },
        {
            "label": "docker push booking service",
            "type": "shell",
            "command": "docker push acrappsaks.azurecr.io/booking-service:v0.2.0",
            "options": {
                "cwd": "${workspaceFolder}/src/booking_service/src"
            }
        }
    ]
}